#include "main.h"
#include <stdio.h>
/**
 * print_unsigned - prints unsigned integer.
 * @n: number
 * @size: size specifier
 * Returns: number of char to print.
 */
int print_unsigned(va_list types, char buff[],
                  int flags, int width, int prec, int size)
{
    unsigned long int n = va_arg(types, unsigned long int);
    n = convert_size_unsgnd(n, size);

    int i = 0;
    do
    {
        buff[i++] = (n % 10) + '0';
        n /= 10;
    } while (n > 0);

    return write_unsgnd(0, i, size, flags, prec, width, buff);
}
